<p id="notice"><%= notice %></p>

<h1>Assignments Student Courses</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assignments_student_courses.each do |assignments_student_course| %>
      <tr>
        <td><%= assignments_student_course.student_id %></td>
        <td><%= assignments_student_course.course_id %></td>
        <td><%= link_to 'Show', assignments_student_course %></td>
        <td><%= link_to 'Edit', edit_assignments_student_course_path(assignments_student_course) %></td>
        <td><%= link_to 'Destroy', assignments_student_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assignments Student Course', new_assignments_student_course_path %>
